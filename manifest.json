{
  "manifest_version": 3,
  "name": "UdemyPlus",
  "version": "1.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.udemy.com/course/*/learn/*"],
      "css": [
        "styles/udemyplus.css",
        "libs/css/bootstrap.min.css",
        "libs/css/fontawesome.min.css",
        "libs/css/poppins.css"
      ],
      "js": [
        "libs/js/html2canvas.min.js",
        "libs/js/interact.min.js",
        "dist/course-page.bundle.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "libs/css/fontawesome.min.css",
        "libs/webfonts/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
